{
  "name": "front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deps-update": "ncu --deep --color --interactive",
    "build": "wireit",
    "build:backend": "wireit",
    "build:front": "wireit",
    "dev": "wireit",
    "dev:backend": "wireit",
    "dev:front": "wireit",
    "format": "wireit",
    "format:prettier": "wireit",
    "format:sort": "wireit",
    "front:analyze": "wireit",
    "front:build": "wireit",
    "front:build:vite": "wireit",
    "front:dev": "wireit",
    "lint": "wireit",
    "preview": "wireit",
    "sort": "wireit",
    "test": "wireit",
    "typecheck": "wireit"
  },
  "devDependencies": {
    "@types/lodash": "4.14.182",
    "@types/ua-parser-js": "0.7.36",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "npm-check-updates": "13.0.1",
    "prettier": "2.6.2",
    "rollup": "2.74.1",
    "rollup-plugin-visualizer": "5.6.0",
    "sort-package-json": "1.57.0",
    "typescript": "4.6.4",
    "wireit": "0.4.3"
  },
  "engines": {
    "node": "18.2.0",
    "pnpm": "7.1.2"
  },
  "wireit": {
    "analyze": {
      "command": "vite build --mode analyze",
      "dependencies": [
        "typecheck"
      ]
    },
    "build": {
      "dependencies": [
        "build:backend",
        "build:front"
      ]
    },
    "build:backend": {
      "dependencies": [
        "./packages/backend:build"
      ]
    },
    "build:front": {
      "dependencies": [
        "./packages/front:build"
      ]
    },
    "dev": {
      "dependencies": [
        "dev:backend",
        "dev:front"
      ]
    },
    "dev:backend": {
      "dependencies": [
        "./packages/backend:dev"
      ]
    },
    "dev:front": {
      "dependencies": [
        "./packages/front:dev"
      ]
    },
    "format": {
      "dependencies": [
        "format:prettier",
        "format:sort"
      ]
    },
    "format:prettier": {
      "command": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'"
    },
    "format:sort": {
      "command": "sort-package-json 'package.json'"
    },
    "lint": {
      "command": "eslint --color --cache --fix packages/**/*.{ts,tsx} --ignore-path ./.gitignore --cache-location .eslintcache",
      "files": [
        "packages/**/*.ts",
        "packages/**/*.tsx",
        ".eslintrc"
      ],
      "output": [
        ".eslintcache"
      ]
    },
    "preview": {
      "command": "vite preview --port 5050"
    },
    "test": {
      "command": "vitest run"
    },
    "typecheck": {
      "command": "tsc --noEmit --pretty",
      "files": [
        "envs/**",
        "src/**/*.{ts,tsx}",
        "*.{js,json,ts,tsx}"
      ],
      "output": []
    }
  }
}
