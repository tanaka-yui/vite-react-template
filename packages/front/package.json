{
  "name": "@vite/front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "wireit",
    "build": "wireit",
    "build:vite": "wireit",
    "dev": "wireit",
    "preview": "wireit",
    "test": "wireit",
    "typecheck": "wireit"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.8.0",
    "@mui/material": "5.8.0",
    "dayjs": "1.11.2",
    "libphonenumber-js": "1.10.4",
    "lodash": "4.17.21",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "react-use": "17.4.0",
    "ua-parser-js": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.0",
    "@vitejs/plugin-react": "1.3.2",
    "happy-dom": "4.0.1",
    "vite": "2.9.9",
    "vitest": "0.12.7"
  },
  "engines": {
    "node": "18.2.0",
    "pnpm": "7.1.2"
  },
  "wireit": {
    "analyze": {
      "command": "vite build --mode analyze",
      "dependencies": [
        "typecheck"
      ]
    },
    "build": {
      "dependencies": [
        "typecheck",
        "build:vite"
      ]
    },
    "build:vite": {
      "command": "vite build",
      "clean": "if-file-deleted",
      "files": [
        "src/**",
        "envs/**",
        "*.{ts,tsx}"
      ],
      "output": [
        "src/dev-dist/**",
        "src/dist/**"
      ]
    },
    "dev": {
      "command": "vite --mode development"
    },
    "preview": {
      "command": "vite preview --port 5050"
    },
    "test": {
      "command": "vitest run"
    },
    "typecheck": {
      "command": "tsc --noEmit --pretty",
      "files": [
        "envs/**",
        "src/**/*.{ts,tsx}",
        "*.{js,json,ts,tsx}"
      ],
      "output": []
    }
  }
}
